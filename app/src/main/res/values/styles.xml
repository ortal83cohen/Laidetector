<resources>

    <!-- Base application theme. -->
    <style name="neutralTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/natural_theme_primary</item>
        <item name="colorPrimaryDark">@color/natural_theme_primary_dark</item>
        <item name="colorAccent">@color/natural_theme_accent</item>
        <item name="android:windowBackground">@color/theme_background</item>
        <item name="android:windowTranslucentStatus">true</item>
    </style>


    <style name="trueTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/true_theme_primary</item>
        <item name="colorPrimaryDark">@color/true_theme_primary_dark</item>
        <item name="colorAccent">@color/true_theme_accent</item>
        <item name="android:windowBackground">@color/theme_background</item>
        <item name="android:windowTranslucentStatus">true</item>
    </style>

    <style name="falseTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/false_theme_primary</item>
        <item name="colorPrimaryDark">@color/false_theme_primary_dark</item>
        <item name="colorAccent">@color/false_theme_accent</item>
        <item name="android:windowBackground">@color/theme_background</item>
        <item name="android:windowTranslucentStatus">true</item>
    </style>

</resources>
